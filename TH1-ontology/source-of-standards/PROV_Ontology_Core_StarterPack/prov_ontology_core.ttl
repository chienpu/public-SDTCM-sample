@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.w3.org/ns/prov> a owl:Ontology ; rdfs:label 'PROV-O Extract (Core Terms)' .

prov:Entity a owl:Class ; rdfs:comment "Thing with some fixed aspects; may be physical, digital, or conceptual." .
prov:Activity a owl:Class ; rdfs:comment "Something that occurs over time and acts upon entities." .
prov:Agent a owl:Class ; rdfs:comment "Something that bears responsibility for an activity taking place." .
prov:Plan a owl:Class ; rdfs:comment "A plan that guides the execution of an activity." .
prov:Role a owl:Class ; rdfs:comment "The function of an entity or agent in an activity." .
prov:Collection a owl:Class ; rdfs:comment "A set of entities." .
prov:Bundle a owl:Class ; rdfs:comment "A named set of provenance descriptions." .
prov:Location a owl:Class ; rdfs:comment "A place or region." .
prov:Generation a owl:Class ; rdfs:comment "An instantaneous event marking entity creation." .
prov:Usage a owl:Class ; rdfs:comment "An event of an entity being used." .
prov:Invalidation a owl:Class ; rdfs:comment "Event marking entity invalidation." .
prov:Start a owl:Class ; rdfs:comment "Event marking activity start." .
prov:End a owl:Class ; rdfs:comment "Event marking activity end." .
prov:Communication a owl:Class ; rdfs:comment "Event marking information flow between activities." .
prov:Association a owl:Class ; rdfs:comment "Links an activity to an agent." .
prov:Attribution a owl:Class ; rdfs:comment "Assigns credit for an entity to an agent." .
prov:Delegation a owl:Class ; rdfs:comment "Delegation relation between agents." .
prov:Derivation a owl:Class ; rdfs:comment "Indicates entity derivation." .
prov:Influence a owl:Class ; rdfs:comment "Superclass of influence relations." .
prov:wasGeneratedBy a owl:ObjectProperty ; rdfs:domain prov:Entity ; rdfs:range prov:Activity ; rdfs:comment "Links an entity to the activity that generated it." .
prov:used a owl:ObjectProperty ; rdfs:domain prov:Activity ; rdfs:range prov:Entity ; rdfs:comment "Indicates an activity used an entity." .
prov:wasInformedBy a owl:ObjectProperty ; rdfs:domain prov:Activity ; rdfs:range prov:Activity ; rdfs:comment "One activity informs another." .
prov:wasStartedBy a owl:ObjectProperty ; rdfs:domain prov:Activity ; rdfs:range prov:Entity/Activity ; rdfs:comment "Activity start trigger." .
prov:wasEndedBy a owl:ObjectProperty ; rdfs:domain prov:Activity ; rdfs:range prov:Entity/Activity ; rdfs:comment "Activity end trigger." .
prov:wasInvalidatedBy a owl:ObjectProperty ; rdfs:domain prov:Entity ; rdfs:range prov:Activity ; rdfs:comment "Entity became invalid due to an activity." .
prov:wasAssociatedWith a owl:ObjectProperty ; rdfs:domain prov:Activity ; rdfs:range prov:Agent ; rdfs:comment "Associates an agent with an activity." .
prov:wasAttributedTo a owl:ObjectProperty ; rdfs:domain prov:Entity ; rdfs:range prov:Agent ; rdfs:comment "Credits an agent for an entityâ€™s existence." .
prov:actedOnBehalfOf a owl:ObjectProperty ; rdfs:domain prov:Agent ; rdfs:range prov:Agent ; rdfs:comment "Delegation between agents." .
prov:wasDerivedFrom a owl:ObjectProperty ; rdfs:domain prov:Entity ; rdfs:range prov:Entity ; rdfs:comment "Derivation between entities." .
prov:alternateOf a owl:ObjectProperty ; rdfs:domain prov:Entity ; rdfs:range prov:Entity ; rdfs:comment "Indicates alternative representations." .
prov:specializationOf a owl:ObjectProperty ; rdfs:domain prov:Entity ; rdfs:range prov:Entity ; rdfs:comment "Indicates specialization hierarchy." .
prov:hadMember a owl:ObjectProperty ; rdfs:domain prov:Collection ; rdfs:range prov:Entity ; rdfs:comment "Collection membership." .
prov:hadPlan a owl:ObjectProperty ; rdfs:domain prov:Association ; rdfs:range prov:Plan ; rdfs:comment "Plan followed in an activity." .
prov:hadRole a owl:ObjectProperty ; rdfs:domain prov:Association ; rdfs:range prov:Role ; rdfs:comment "Role of agent or entity." .
prov:atLocation a owl:ObjectProperty ; rdfs:domain prov:Entity/Activity ; rdfs:range prov:Location ; rdfs:comment "Spatial context." .
prov:startedAtTime a owl:DatatypeProperty ; rdfs:domain prov:Activity ; rdfs:range xsd:dateTime ; rdfs:comment "Start time of activity." .
prov:endedAtTime a owl:DatatypeProperty ; rdfs:domain prov:Activity ; rdfs:range xsd:dateTime ; rdfs:comment "End time of activity." .
prov:generatedAtTime a owl:DatatypeProperty ; rdfs:domain prov:Entity ; rdfs:range xsd:dateTime ; rdfs:comment "Time entity was generated." .
prov:invalidatedAtTime a owl:DatatypeProperty ; rdfs:domain prov:Entity ; rdfs:range xsd:dateTime ; rdfs:comment "Time entity was invalidated." .
prov:value a owl:DatatypeProperty ; rdfs:domain prov:Entity ; rdfs:range rdfs:Literal ; rdfs:comment "Literal value held by an entity." .
prov:atTime a owl:DatatypeProperty ; rdfs:domain prov:InstantaneousEvent ; rdfs:range xsd:dateTime ; rdfs:comment "Timestamp for an event." .